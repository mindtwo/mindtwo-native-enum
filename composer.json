{
    "name": "mindtwo/native-enum",
    "type": "library",
    "description": "Package for using native php enums.",
    "keywords": [
        "php",
        "enum",
        "php 8.1",
        "laravel-enum",
        "laravel",
        "illuminate",
        "native-enum"
    ],
    "homepage": "https://github.com/mindtwo/native-enum",
    "license": "MIT",
    "authors": [
        {
            "name": "mindtwo GmbH",
            "email": "info@mindtwo.de",
            "homepage": "https://www.mindtwo.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.1",
        "illuminate/collections": "^8.82",
        "illuminate/contracts": "^8.82",
        "illuminate/support": "^8.82",
        "illuminate/translation": "^8.82"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10",
        "squizlabs/php_codesniffer": "^3.6.2"
    },
    "autoload": {
        "psr-4": {
            "mindtwo\\NativeEnum\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "mindtwo\\NativeEnum\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
